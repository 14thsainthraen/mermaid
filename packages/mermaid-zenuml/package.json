  "keywords": [
    "diagram",
    "markdown",
    "zenuml",
    "mermaid"
  ],
  "scripts": {
    "prepublishOnly": "pnpm -w run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mermaid-js/mermaid"
  },
  "author": "Peng Xiao",
  "license": "MIT",
  "standard": {
    "ignore": [
      "dist/**/*.js"
    ],
    "globals": [
      "page"
    ]
  },
  "dependencies": {
    "@zenuml/core": "^2.0.36"
  },
  "devDependencies": {
    "mermaid": "workspace:*"
  },
  "files": [
    "dist"
  ]
