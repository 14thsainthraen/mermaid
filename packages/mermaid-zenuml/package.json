{
  "name": "@mermaid-js/mermaid-zenuml",
  "version": "0.0.1",
  "description": "Zenuml integration for MermaidJS.",
  "module": "dist/mermaid-zenuml.core.mjs",
  "types": "dist/detector.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/mermaid-zenuml.core.mjs",
      "types": "./dist/detector.d.ts"
    },
    "./*": "./*"
