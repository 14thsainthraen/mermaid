{
  "name": "@mermaid-js/mermaid-timeline",
  "version": "9.4.0",
  "description": "Timeline diagram module for Mermaid.js.",
  "module": "dist/mermaid-timeline.core.mjs",
  "types": "dist/detector.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/mermaid-timeline.core.mjs",
      "types": "./dist/detector.d.ts"
    },
    "./*": "./*"
  },
  "keywords": [
    "diagram",
    "markdown",
    "timeline",
    "mermaid"
  ],
  "scripts": {
    "prepublishOnly": "pnpm -w run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mermaid-js/mermaid"
  },
  "author": "Ashish Jain",
  "license": "MIT",
  "standard": {
    "ignore": [
      "**/parser/*.js",
      "dist/**/*.js",
      "cypress/**/*.js"
    ],
    "globals": [
      "page"
    ]
  },
  "dependencies": {
    "d3": "^7.0.0",
    "khroma": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "rimraf": "^3.0.2"
  },
  "resolutions": {
    "d3": "^7.0.0"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ]
}